{
  "version": "3.0.0",
  "name": "david-wells-io",
  "description": "Davids site",
  "homepage": "https://davidwells.io",
  "main": "n/a",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build --log-pages",
    "serve": "http-server public",
    "deploy": "netlify deploy -p --dir public",
    "postbuild": "cp -R resume public && node scripts/sitemap.js",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'"
  },
  "repository": "git+https://github.com/davidwells/davidwells.io.git",
  "keywords": [
    "gatsby",
    "blog",
    "react",
    "theme",
    "starter"
  ],
  "author": "David Wells <hello@davidwells.io>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "clipboard": "^1.7.1",
    "date-fns": "^1.30.1",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-external-links": "0.0.4",
    "get-form-data": "^2.0.0",
    "lodash": "^4.17.15",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gatsby": "^2.20.35",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "2.2.1",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.2",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.20",
    "icon-pipeline": "0.0.5",
    "markdown-magic": "^1.0.0",
    "netlify-plugin-gatsby-cache": "^0.2.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^1.10.2"
  }
}
